<?xml version='1.0' encoding='utf-8'?>
<widget id="senola.github.com" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>潭风的博客</name>
    <description>
       潭风的博客.
    </description>
    <author email="xinshouzhilu@163.com" href="senola.github.io">
        senola 
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="InAppBrowserStorageEnabled" value="true" />
    <preference name="LoadingPageDialog" value="潭风提示你,正在加载中..." />
    <preference name="ErrorUrl" value="index.html" />
    <preference name="ShowTitle" value="true" />
    <preference name="AndroidLaunchMode" value="singleTop" />
</widget>
